{
  "name": "logicstamp-mcp",
  "version": "0.1.6",
  "description": "MCP server for LogicStamp Context - analyze React/TypeScript codebases",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "logicstamp-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=e2e"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "logicstamp",
    "react",
    "typescript",
    "codebase-analysis",
    "ai-tools"
  ],
  "author": "Amit Levi <logicstamp.dev@gmail.com> (https://amitlevi.dev)",
  "contributors": [
    "LogicStamp Contributors"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LogicStamp/logicstamp-mcp.git"
  },
  "homepage": "https://github.com/LogicStamp/logicstamp-mcp#readme",
  "bugs": {
    "url": "https://github.com/LogicStamp/logicstamp-mcp/issues"
  },
  "files": [
    "dist",
    "docs/logicstamp-for-llms.md",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    ".claude.json.example",
    ".mcp.json.example"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
